<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="orc.com.taotaole">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".main.Activity.DrawActivity"
            android:label="@string/title_activity_draw"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".main.Activity.ForgetActivity" />
        <activity android:name=".main.Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.Activity.RegisterActivity" />
        <activity android:name=".main.Activity.MainActivity" />
        <activity android:name=".main.Activity.GoodsActivity" />
        <activity android:name=".main.Activity.DetailActivity" />
        <activity android:name=".main.Activity.HistoryActivity"
            android:parentActivityName=".main.Activity.MainActivity"/>
    </application>

</manifest>